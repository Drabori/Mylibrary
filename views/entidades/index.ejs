<!--views\entidades\index.ejs-->
<h2>Search Entidades</h2>
<form action="/entidades" method="GET">
  <label>Name</label>
  <input type="text" name="name" value="<%= searchOptions.name %> " />
  <button type="submit">Search</button>
</form>
<br />
<!-- Exibir cada entidade-->
<div class="entidades-container">
  <% entidades.forEach (entidade => { %>
    <div class="entidade"></div>
     <%= entidade.name %>
  <img height="150" width="100" src="<%= entidade.coverImagePath %>" />
  <% }) %>
</div>
